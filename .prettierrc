{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "experimentalBabelParserPluginsList": ["jsx", "typescript"],
  "importOrder": [
    "^react$",
    "^react-dom$",
    "^next.*",
    "^(?![.~]).*(?<!.css|.scss)$",
    "^~.*(?<!.css|.scss)$",
    "^\\..*(?<!.css|.scss)$",
    "^(?![.~]).*$",
    "^~.*",
    "^\\..*",
    ".*"
  ]
}
